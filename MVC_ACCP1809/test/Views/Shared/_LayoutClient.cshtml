    <script src="@Url.Content("~/content/client/")assets/js/nagging-menu.js"></script>
    <script src="@Url.Content("~/content/client/")assets/js/jquery.nav.js"></script>
    <script src="@Url.Content("~/content/client/")assets/js/prettyPhoto/jquery.prettyPhoto.js"></script>
    <script src="@Url.Content("~/content/client/")assets/js/portfolio/jquery.quicksand.js"></script>
    <script src="@Url.Content("~/content/client/")assets/js/portfolio/setting.js"></script>
    <script src="@Url.Content("~/content/client/")assets/js/hover/jquery-hover-effect.js"></script>
    <script src="@Url.Content("~/content/client/")/js/jquery.scrollTo.min.js"></script>
    <script src="@Url.Content("~/content/client/")assets/js/animate.js"></script>

    <!-- Contact Form JavaScript File -->
    <script src="@Url.Content("~/content/client/")contactform/contactform.js"></script>

    <!-- Template Custom Javascript File -->
    <script src="@Url.Content("~/content/client/")assets/js/custom.js"></script>

</body>
</html>
